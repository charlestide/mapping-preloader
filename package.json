{
  "name": "mapping-preloader",
  "version": "0.0.1",
  "description": "a module-loader for javascript that sync loading all modules in the mapping",
  "main": "dist/index.js",
  "files": [
    "dist/*.js",
    "types/*.d.ts"
  ],
  "bin": "dist/preload.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --require ts-node/register ./test/**.tsx",
    "dev": "cross-env NODE_ENV=dev tsc -w",
    "build": "cross-env NODE_ENV=prod tsc --listFiles --listEmittedFiles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charlestide/mapping-preloader.git"
  },
  "keywords": [
    "javascript",
    "modules",
    "import"
  ],
  "author": {
    "name": "Tangkeyu",
    "email": "charlestide@vip.163.com"
  },
  "license": "MIT",
  "dependencies": {
    "@types/defaults": "^1.0.2",
    "@types/fs-extra": "^5.0.0",
    "@types/graceful-fs": "^4.1.2",
    "@types/lodash": "^4.14.92",
    "@types/lodash.defaults": "^4.2.3",
    "@types/pathjs": "^0.8.33",
    "@types/webpack": "^3.8.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "colors": "^1.1.2",
    "commander": "^2.13.0",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.4",
    "mocha": "^4.1.0",
    "path": "^0.12.7",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/core-js": "^0.9.44",
    "@types/jsonfile": "^4.0.1",
    "@types/mocha": "^2.2.46",
    "@types/node": "^8.5.8",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "node": "^8.9.4",
    "ts-node": "^4.1.0",
    "webpack": "^3.10.0"
  }
}
